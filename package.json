{
  "name": "scrapely",
  "license": "MIT",
  "version": "0.0.3",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "description": "The most simple JavaScript scraping framework",
  "private": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "url": "https://github.com/genkoph/scrapely"
  },
  "scripts": {
